{
  "manifest_version": 2,

  "name": "Mavrck Marktr",
  "description": "Pulls data from instagram profiles",
  "version": "0.0.2",

  "browser_action": {
    "default_popup": "popup.html",
  	"default_title": "Mavrck IG-Tool",
  	"default_icon": "popup.png"
  },

  "permissions": [
	   "notifications",
     "activeTab",
     "storage",
     "identity",
     "tabs",
     "http://*.app.splashscore.com/*/"
  ]
}
