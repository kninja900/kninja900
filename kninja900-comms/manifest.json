{
  "manifest_version": 2,

  "name": "Mavrck Marktr",
  "description": "Pulls data from instagram profiles",
  "version": "2.0",
  "key": "kopnmhmejhkgjhkpiblenbemcednhako",

  "browser_action": {
    "default_popup": "popup.html",
  	"default_title": "Mavrck IG-Tool",
  	"default_icon": "popup.png"
  },

  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "myScript.js",
      "popup.js"
    ]
  },

  "permissions": [
	   "notifications",
     "activeTab",
     "storage",
     "identity",
     "tabs"
  ],

   "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": [
        "jquery-3.2.1.min.js",
        "myScript.js"
      ]
    }
  ]
}
